<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>mozilla-capi@bklosr.de</em:id>
    <em:name>MS-CAPI PKCS#11 Installer</em:name>
    <em:description>Installs MS-CAPI PKCS#11 module in Firefox or Thunderbird with the "Friendly" flag set from https://www.risacher.org/p11-capi/</em:description>
    <em:version>0.1</em:version>
    <em:creator>bklosr.de</em:creator>
    <em:developer>Jan-Philipp Huelshoff</em:developer>
    <em:homepageURL>https://github.com/janhf/mozilla-capi/</em:homepageURL>
    <em:type>2</em:type>
    <em:unpack>true</em:unpack>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>5.0</em:minVersion>
        <em:maxVersion>10.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Mozilla Thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>5.0</em:minVersion>
        <em:maxVersion>10.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
